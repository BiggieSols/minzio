<div class="group" data-id="<%= group.id %>">
  <div class="group-name">
    <%= group.get("name") %>
  </div>
  <div class="group-edit dropdown">
    <a class="modify-group-button" data-toggle="dropdown">
      <span class="glyphicon glyphicon-pencil dropdown-toggle"></span>
    </a>
      <ul class="dropdown-menu dropdown-menu-right" role="menu">
        <li role="presentation" class="edit-button"><a role="menuitem" tabindex="-1">Edit</a></li>
        <li role="presentation"><a role="menuitem" tabindex="-1">Delete</a></li>
      </ul>
    </div>
  </div>
</div>
