<div class="row">
  <div class="col-xs-12 col-sm-5 col-sm-offset-1">
    <div class="row content-area user-top">
      <div class="col-xs-12 col-sm-6">
        <img src="<%= user.get("large_image_url") %>" class="user-image">
      </div>
      <div class="col-xs-12 col-sm-5 col-sm-offset-1">
        <div class="profile-item user-name">
          <%= user.escape("name") %>
        </div>
        <div class="profile-item">
          <%= user.escape("headline") %>
        </div>
        <div class="profile-item">
          <%= user.escape("industry") %>
        </div>
        <div class="profile-item">
          <%= user.escape("location") %>
        </div>


        <br>
      </div>
    </div>
    <div class="row">
      <br>
      <div id="results-chart" class="content-area">
      </div>
    </div>
  </div>
  <div class="col-xs-12 col-sm-5 content-area personality-column">
    <div class="personality-type">
      <%= user.get("personality_type").escape("title") %>
    </div>
    <div class="long-personality-type">
      <%= user.get("personality_type").escape("small_info") %>
    </div>
    <div class="personality-info lead">
      <%= user.get("personality_type").escape("large_info") %>
    </div>
    <div class="working-with-personality">
      <ul class="nav nav-pills nav-justified">
        <li class="active switch" data-category="colleague"><a>As Colleagues</a></li>
        <li class="switch" data-category="manager"><a>As Managers</a></li>
        <li class="switch" data-category="employee"><a>As Employees</a></li>
      </ul>
      <div class="traits-table">
      </div>
    </div>
  </div>
</div>